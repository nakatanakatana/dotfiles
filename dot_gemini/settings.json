      "GEMINI.md"
    ]
  },
  "tools": {
    "allowed": [
      "glob",
      "google_web_search",
      "read_file",
      "list_directory",
      "search_file_content",
      "web_fetch",
      "replace",
      "write_file",
      "run_shell_command(ls)",
      "run_shell_command(mkdir)",
